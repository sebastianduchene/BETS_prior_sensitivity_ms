rescale_mles <- function(mle_values){
mle_values <- mle_values - max(mle_values)
mle_values <- mle_values + 10
mle_values[mle_values < -10 ] <- 1
mle_values[mle_values == 1] <- rnorm(sum(mle_values == 1), 1, 0.2)
return(mle_values)
}
